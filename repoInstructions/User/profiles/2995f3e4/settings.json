{
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "file",
    "eslint.format.enable": true,
    "eslint.useFlatConfig": true,
    "eslint.validate": [
        "dockercompose",
        "javascript",
        "json",
        "json5",
        "jsonc",
        "markdown",
        "md",
        "mdx",
        "yaml",
        "yml"
    ],
    "files.autoSave": "onFocusChange"
}
